<p id="notice"><%= notice %></p>

<h1>Type Devices</h1>

<table>
  <thead>
    <tr>
      <th>Devices</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_devices.each do |type_device| %>
      <tr>
        <td><%= type_device.devices %></td>

        <td> <%= type_device.devices.name %></td>


        <td><%= link_to 'Show', type_device %></td>
        <td><%= link_to 'Edit', edit_type_device_path(type_device) %></td>
        <td><%= link_to 'Destroy', type_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Device', new_type_device_path %>
